// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Personaldata {
  id         Int    @id @default(autoincrement())
  first_name String
  last_name  String
  email      String
  gender     String
  ip_address String
}
